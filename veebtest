package tanelesmaspaev;

import static com.codeborne.selenide.Selenide.*;
import static com.codeborne.selenide.Condition.*;
import org.openqa.selenium.By;

public class Test1{
	public static void main(String[] args){
		open("http://greeny.cs.tlu.ee/~kukltane/Java/Kodut%C3%B6%C3%B6d/t09veebtest/determinant.html");
		$(By.id("field1")).setValue("5");
		$(By.id("field2")).setValue("1");
		$(By.id("field3")).setValue("3");
		$(By.id("field4")).setValue("2");
		$("#calculate").click();
		System.out.println("Vastus: "+$("#answer").getValue());
	}
}
