</head>
	
<body>
    <input id="field1" type="text">
    <input id="field2" type="text">
    <input id="field3" type="text">
    <input id="field4" type="text">
	
	<button id="calculate" type="button" onclick="calculate()">=</button>
	
	<input id="answer" type="text">

</body>

<script>

window.onload = function () {
	document.getElementById("answer").style.visibility = "hidden";
}

function calculate() {
	document.getElementById("answer").style.visibility = "visible";
    document.getElementById("answer").value = parseInt(document.getElementById("field1").value)*
    parseInt(document.getElementById("field4").value)-parseInt(document.getElementById("field2").value)*
    parseInt(document.getElementById("field3").value);
}

</script>

